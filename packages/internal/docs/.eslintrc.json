{
  "extends": ["../../../.eslintrc.json"],
  "ignorePatterns": [
    "!**/*",
    "node_modules/**/*",
    "build/**/*",
    ".docusaurus/**/*",
    "cdk.out/**/*"
  ],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "import/no-unresolved": [2, { "ignore": ["^@docusaurus", "^@theme"] }]
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
